# Script to mount Windows shares.

mount -t cifs //192.168.0.5/LarryDocs /mnt/larry_docs -ousername=Larry,password=$1,rw,iocharset=utf8,file_mode=0777,dir_mode=0777
mount -t cifs //192.168.0.5/LarryExtDocs /mnt/larry_ext_docs -ousername=Larry,password=$1,rw,iocharset=utf8,file_mode=0777,dir_mode=0777


